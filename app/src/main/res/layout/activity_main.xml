<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >


    <Button
        android:id="@+id/buttongo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="60dp"
        android:text="浏览网页" />
    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/browse_view"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="222dp"
        android:layout_marginTop="-45dp"
        fresco:placeholderImage="@mipmap/place_img"
        />
    <Button
        android:id="@+id/buttonfm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="90dp"
        android:text="帧测试"/>
    <com.example.homework1.myview.CircularView
        android:id="@+id/cirview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginTop="200dp"
        app:textContent="testing"
        app:textColor="@color/black"
        app:textSize="10sp"
        app:circleColor="@color/teal_200"/>


</LinearLayout>